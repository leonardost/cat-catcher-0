function cat_actor(floor)
  local life = 1
  local cat1 = sprite(1, 1, { 1 })
  local cat2 = sprite(1, 1, { 2 })
  local animation = animation()
  animation.add_frame(cat1, 8)
  animation.add_frame(cat2, 8)
  local walking = state(
  	 "walking",
  	 animation
  )
  local cat = actor(
    128,
    floor_base_y + 8 + floor * 32,
    { walking }
  )
  
  function cat.update()
    cat.update_state()
    cat.x -= 1
    if cat.x < -8 then
      cat.x = 128
    end
  end

  function cat.caught()
    life -= 1
    if life == 0 then
      sfx(8)
      return true
    end
    return false
  end
  
  cat.is_cat = true
  cat.floor = floor
	 cat.is_vulnerable = true
  
  return cat
end

